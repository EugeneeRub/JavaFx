<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoadFromFileController">
    <children>
        <Label alignment="CENTER" text="Запись из файла" />
        <TextField fx:id="tfLoadFromFile" promptText="Введите путь">
            <VBox.margin>
                <Insets bottom="5.0" top="5.0" />
            </VBox.margin>
        </TextField>
        <Button fx:id="btnLoadFromFile" mnemonicParsing="false" onAction="#loadFromFile" text="Прочесть" />
    </children>
    <padding>
        <Insets top="5.0" />
    </padding>
</VBox>
